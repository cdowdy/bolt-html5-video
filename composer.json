{
  "name": "cdowdy/bolt-html5-video",
  "description": "A Bolt Extension to Add Local or CDN HTML5 Videos or Webm/Mp4 for gif like animations.",
  "type": "bolt-extension",
  "keywords": [
    "html5 video",
    "Save-Data Header",
    "Client Hints",
    "webm",
    "mp4",
    "gif",
    "gifs",
    "webm for gifs",
    "mp4 for gifs",
    "ogg",
    "local videos",
    "cdn hosted videos"
  ],
  "require": {
    "bolt/bolt": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Cory Dowdy",
      "email": "cory@corydowdy.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Bolt\\Extension\\cdowdy\\html5video\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bolt\\Extension\\cdowdy\\html5video\\Tests\\": "tests",
      "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
    }
  },
  "extra": {
    "bolt-assets": "web",
    "bolt-class": "Bolt\\Extension\\cdowdy\\html5video\\Html5VideoExtension",
    "bolt-icon": "screenshots/html5-videos-icon.png",
    "bolt-screenshots": [
      "screenshots/contenttypes-config.png",
      "screenshots/template-usage.png",
      "screenshots/template-single-output.png",
      "screenshots/output-code.png"
    ]
  }
}
